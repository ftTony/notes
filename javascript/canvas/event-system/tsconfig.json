{
    "compilerOptions": {
      "target": "es2015",
      "module": "ESNext",
      "jsx": "react",
      "jsxFactory": "h",
      "importHelpers": true,
      "moduleResolution": "node",
      "declaration": true,
      "allowSyntheticDefaultImports": true,
      //json
